        },
        "3": {
          "name": "entity.name.function.directive.archive.hyperbook"
        },
        "4": {
          "name": "string.unquoted"
        },
        "5": {
          "patterns": [
            {
              "match": "(name)=\"([^\"]*)\"",
              "captures": {
                "1": {
                  "name": "keyword.parameter.directive.archive.hyperbook"
                },
                "2": {
                  "name": "string.quoted.double.untitled"
